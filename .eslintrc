{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-trailing-spaces": [
      "error",
      {
        "skipBlankLines": true
      }
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "log",
          "info",
          "warn",
          "error"
        ]
      }
    ]
  }
}